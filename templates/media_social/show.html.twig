

{% extends 'admin_base.html.twig' %}

{% block main %}
    <h1>MediaSocial</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ media_social.id }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ media_social.link }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ media_social.image }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ media_social.title }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ media_social.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_media_social_index') }}">back to list</a>

    <a href="{{ path('app_media_social_edit', {'id': media_social.id}) }}">edit</a>

    {{ include('media_social/_delete_form.html.twig') }}
{% endblock %}
